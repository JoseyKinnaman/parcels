<h1>Update <%= @parcel.length  @parcel.width @parcel.height%></h1>

<form action="/parcels/<%= @parcel.id() %>" method="post">
  <input name="_method" type="hidden" value="patch">

  <label for="name">Enter new dimensions</label>
  <input id="name" name="length" type="text">
  <input id="name" name="width" type="text">
  <input id="name" name="height" type="text">

  <button type="submit">Update</button>
</form>

<p><a href="/parcels">Return to package list</a></p>